import request from '../utils/request'

export function specifyRole(data) {
  return request({
    url: '/SuperAdmin/specifyRole',
    method: 'post',
    data
  })
}


export function roleList(data) {
  return request({
    url: '/SuperAdmin/roleList',
    method: 'post',
    data
  })
}


export function userList(data) {
  return request({
    url: '/SuperAdmin/userList',
    method: 'post',
    data
  })
}

export function getInfo(token) {
  return request({
    url: '/vue-admin-template/user/info',
    method: 'get',
    params: { token }
  })
}

export function logout() {
  return request({
    url: '/vue-admin-template/user/logout',
    method: 'post'
  })
}
